#include <stdio.h>
int main()
{
	int rows, i, j, p, q, k, test_count, count=0;
	scanf("%d", &test_count);
	for(k=0; k<test_count; k++){
		scanf("%d", &rows);
		int a[rows][rows];
		for(i=0; i<rows; i++)
			for(j=0; j<rows; j++)
				scanf("%d",&a[i][j]);
		for(i=0; i<rows; i++)
			for(j=0; j<rows; j++)
				for(p=i; p<rows; p++)
					for(q=j; q<rows; q++)
						if(a[i][j]>a[p][q])
							count++;
		printf("%d\n", count);
	}
}
